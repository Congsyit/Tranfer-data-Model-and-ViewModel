<UserControl x:Class="TranferData2Form2.View.UserControl1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TranferData2Form2.View"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <TextBox Text="{Binding fSize.Value, UpdateSourceTrigger=PropertyChanged}" Margin="20" Background="AntiqueWhite"></TextBox>
        <TextBox Text="{Binding Text.Value, UpdateSourceTrigger=PropertyChanged}" Margin="20" Grid.Row="1" Background="AliceBlue"></TextBox>
    </Grid>
</UserControl>
